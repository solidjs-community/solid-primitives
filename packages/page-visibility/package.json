{
  "name": "@solid-primitives/page-visibility",
  "version": "2.1.0",
  "description": "Primitive to track page visibility",
  "author": "David Di Biase",
  "contributors": [
    "Damian Tarnawski <gthetarnav@gmail.com>",
    "Tom Pichaud <dev.tompichaud@icloud.com>"
  ],
  "license": "MIT",
  "homepage": "https://primitives.solidjs.community/package/page-visibility",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidjs-community/solid-primitives.git"
  },
  "primitive": {
    "name": "page-visibility",
    "stage": 3,
    "list": [
      "createPageVisibility"
    ],
    "category": "Display & Media"
  },
  "files": [
    "dist"
  ],
  "private": false,
  "sideEffects": false,
  "type": "module",
  "module": "./dist/index.js",
  "browser": {},
  "types": "./dist/index.d.ts",
  "exports": {
    "@solid-primitives/source": "./src/index.ts",
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "tsx ../../scripts/dev.ts",
    "build": "tsx ../../scripts/build.ts",
    "vitest": "vitest -c ../../configs/vitest.config.ts",
    "test": "pnpm run vitest",
    "test:ssr": "pnpm run vitest --mode ssr",
    "test:watch": "watchlist src test -- npm test"
  },
  "keywords": [
    "page",
    "visibility",
    "solid",
    "primitives"
  ],
  "dependencies": {
    "@solid-primitives/event-listener": "workspace:^",
    "@solid-primitives/rootless": "workspace:^",
    "@solid-primitives/utils": "workspace:^"
  },
  "peerDependencies": {
    "solid-js": "^1.6.12"
  },
  "typesVersions": {},
  "devDependencies": {
    "solid-js": "^1.8.7"
  }
}
