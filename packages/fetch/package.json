{
  "name": "@solid-primitives/fetch",
  "version": "1.1.0",
  "description": "Primitive that wraps fetch requests",
  "author": "Alex Lohr <alex.lohr@logmein.com>",
  "license": "MIT",
  "homepage": "https://github.com/solidjs-community/solid-primitives/tree/main/packages/fetch",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidjs-community/solid-primitives.git"
  },
  "primitive": {
    "name": "fetch",
    "stage": 3,
    "list": [
      "createFetch"
    ],
    "category": "Network"
  },
  "files": [
    "dist"
  ],
  "private": false,
  "sideEffects": true,
  "type": "module",
  "main": "./dist/server.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "node": {
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    },
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "clean": "rimraf dist/",
    "build": "tsup",
    "test": "uvu -r solid-register -r test/setup.ts -i test/setup.ts"
  },
  "keywords": [
    "fetch",
    "request",
    "network",
    "solid",
    "primitives"
  ],
  "devDependencies": {
    "@types/node": "^17.0.23",
    "jsdom": "^19.0.0",
    "prettier": "^2.0.5",
    "solid-register": "^0.1.5",
    "tslib": "^2.3.1",
    "tsup": "^5.10.1",
    "typescript": "^4.5.2",
    "uvu": "^0.5.2"
  },
  "peerDependencies": {
    "node-fetch": ">=2.0.0",
    "solid-js": ">=1.3.0"
  },
  "peerDependenciesMeta": {
    "node-fetch": {
      "optional": true
    }
  }
}
