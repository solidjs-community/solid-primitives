{
  "name": "@solid-primitives/range",
  "version": "0.1.13",
  "description": "Control Flow Primitives for displaying given number or a number range of elements.",
  "author": "Damian Tarnawski @thetarnav <gthetarnav@gmail.com>",
  "contributors": [],
  "license": "MIT",
  "homepage": "https://github.com/solidjs-community/solid-primitives/tree/main/packages/range#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidjs-community/solid-primitives.git"
  },
  "bugs": {
    "url": "https://github.com/solidjs-community/solid-primitives/issues"
  },
  "primitive": {
    "name": "range",
    "stage": 1,
    "list": [
      "repeat",
      "mapRange",
      "indexRange",
      "Repeat",
      "Range",
      "IndexRange"
    ],
    "category": "Control Flow"
  },
  "keywords": [
    "control-flow",
    "range",
    "repeat",
    "solid",
    "primitives"
  ],
  "private": false,
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": {},
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "require": "./dist/index.cjs"
  },
  "scripts": {
    "dev": "jiti ../../scripts/dev.ts",
    "build": "jiti ../../scripts/build.ts",
    "vitest": "vitest -c ../../configs/vitest.config.ts",
    "test": "pnpm run vitest",
    "test:ssr": "pnpm run vitest --mode ssr"
  },
  "devDependencies": {
    "solid-transition-group": "^0.2.2"
  },
  "dependencies": {
    "@solid-primitives/utils": "workspace:^"
  },
  "peerDependencies": {
    "solid-js": "^1.6.12"
  },
  "typesVersions": {}
}
