{
  "name": "@solid-primitives/throttle",
  "version": "1.0.8",
  "description": "Primitive that creates a throttle function",
  "author": "David Di Biase <dave.dibiase@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/davedbase/solid-primitives/tree/main/packages/throttle",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davedbase/solid-primitives.git"
  },
  "primitive": {
    "name": "throttle",
    "stage": 3,
    "list": [
      "createThrottle"
    ],
    "category": "Utilities"
  },
  "files": [
    "dist"
  ],
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "node": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "scripts": {
    "start": "vite -r ./dev/ -c ./dev/vite.config.ts",
    "build": "tsup",
    "test": "jest"
  },
  "keywords": [
    "throttle",
    "solid",
    "primitives"
  ],
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "jest": "^27.2.5",
    "prettier": "^2.0.5",
    "solid-testing-library": "^0.2.0",
    "ts-jest": "^27.0.5",
    "tslib": "^2.0.1",
    "tsup": "^5.10.0",
    "typescript": "^4.0.2"
  },
  "peerDependencies": {
    "solid-js": "1.3.0-rc.4"
  },
  "jest": {
    "preset": "ts-jest",
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    },
    "setupFiles": [
      "./test/setup.ts"
    ],
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "solid-js/web": "<rootDir>/node_modules/solid-js/web/dist/web.cjs",
      "solid-js": "<rootDir>/node_modules/solid-js/dist/solid.cjs"
    }
  }
}