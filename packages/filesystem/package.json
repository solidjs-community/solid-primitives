{
  "name": "@solid-primitives/filesystem",
  "version": "1.2.0",
  "description": "A primitive for convenient file system access.",
  "author": "Alex Lohr <alex.lohr@logmein.com>",
  "contributors": [],
  "license": "MIT",
  "homepage": "https://github.com/solidjs-community/solid-primitives/tree/main/packages/filesystem#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidjs-community/solid-primitives.git"
  },
  "bugs": {
    "url": "https://github.com/solidjs-community/solid-primitives/issues"
  },
  "primitive": {
    "name": "filesystem",
    "stage": 0,
    "list": [
      "createFileSystem",
      "createSyncFileSystem",
      "createAsyncFileSystem",
      "makeNoFileSystem",
      "makeNoAsyncFileSystem",
      "makeVirtualFileSystem",
      "makeWebAccessFileSystem",
      "makeNodeFileSystem",
      "makeTauriFileSystem",
      "makeChokidarWatcher",
      "rsync"
    ],
    "category": "Display & Media"
  },
  "keywords": [
    "solid",
    "primitives"
  ],
  "private": false,
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "browser": {},
  "types": "./dist/index.d.ts",
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "require": "./dist/index.cjs"
  },
  "scripts": {
    "dev": "jiti ../../scripts/dev.ts",
    "build": "jiti ../../scripts/build.ts --node",
    "vitest": "vitest -c ../../configs/vitest.config.ts",
    "test": "pnpm run vitest",
    "test:ssr": "pnpm run vitest --mode ssr"
  },
  "devDependencies": {
    "@tauri-apps/api": "^1.3.0",
    "@types/wicg-file-system-access": "2020.9.6",
    "chokidar": "^3.5.3",
    "solid-js": "1.7.6"
  },
  "peerDependencies": {
    "chokidar": "^3.5.3",
    "solid-js": "^1.6.12"
  },
  "peerDependenciesMeta": {
    "chokidar": {
      "optional": true
    }
  },
  "typesVersions": {}
}
