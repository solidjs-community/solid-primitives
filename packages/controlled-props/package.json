{
  "name": "@solid-primitives/controlled-props",
  "version": "0.0.12",
  "description": "The primitives in this package allow you to create controlls for component props.",
  "author": "Alex Lohr <alex.lohr@logmein.com>",
  "contributors": [],
  "license": "MIT",
  "homepage": "https://github.com/solidjs-community/solid-primitives/tree/main/packages/controlled-props",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidjs-community/solid-primitives.git"
  },
  "primitive": {
    "name": "controlled props",
    "stage": 3,
    "list": [
      "createControlledProp"
    ],
    "category": "Utilities"
  },
  "files": [
    "dist"
  ],
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": {},
  "typesVersions": {},
  "exports": {
    "solid": "./dist/index.jsx",
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "require": "./dist/index.cjs"
  },
  "scripts": {
    "dev": "jiti ../../scripts/dev.ts",
    "build": "jiti ../../scripts/build.ts",
    "vitest": "vitest -c ../../configs/vitest.config.ts",
    "test": "pnpm run vitest",
    "test:ssr": "pnpm run vitest --mode ssr"
  },
  "keywords": [
    "controlled props",
    "knobs",
    "controls",
    "solid",
    "primitives"
  ],
  "dependencies": {
    "@solid-primitives/utils": "workspace:^"
  },
  "peerDependencies": {
    "solid-js": "^1.6.12"
  },
  "devDependencies": {
    "solid-js": "1.7.9"
  }
}
