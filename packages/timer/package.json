{
  "name": "@solid-primitives/timer",
  "version": "1.3.2",
  "description": "Primitives to manage timeout and interval",
  "author": "David Di Biase <dave.dibiase@gmail.com>",
  "contributors": [
    "Xavier Loh <xavier.loh.yz@gmail.com>",
    "Damian Tarnawski <gthetarnav@gmail.com>"
  ],
  "license": "MIT",
  "homepage": "https://github.com/solidjs-community/solid-primitives/tree/main/packages/timer#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidjs-community/solid-primitives.git"
  },
  "bugs": {
    "url": "https://github.com/solidjs-community/solid-primitives/issues"
  },
  "primitive": {
    "name": "timer",
    "stage": 3,
    "list": [
      "makeTimer",
      "createTimer",
      "createTimeoutLoop",
      "createPolled",
      "createIntervalCounter"
    ],
    "category": "Browser APIs"
  },
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "./dist/server.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "node": {
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    },
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "vite serve dev --host",
    "dev": "npm run start",
    "build": "tsup",
    "test": "vitest"
  },
  "keywords": [
    "setInterval",
    "setTimeout",
    "timer",
    "solid",
    "primitives"
  ],
  "devDependencies": {
    "prettier": "^2.7.1",
    "solid-js": "^1.5.4",
    "tslib": "^2.4.0",
    "tsup": "^6.2.3",
    "typescript": "^4.8.2",
    "unocss": "^0.45.15",
    "vite": "^3.0.9",
    "vite-plugin-solid": "^2.3.0",
    "vitest": "^0.23.0"
  },
  "peerDependencies": {
    "solid-js": "^1.3.0"
  }
}
