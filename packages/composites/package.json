{
  "name": "@solid-primitives/composites",
  "version": "1.0.5",
  "description": "Composable reactive helpers built around watching computation changes.",
  "author": "Damian Tarnawski <gthetarnav@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/davedbase/solid-primitives/tree/main/packages/composites",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davedbase/solid-primitives.git"
  },
  "primitive": {
    "name": "composites",
    "stage": 0,
    "list": [
      "createCompositeEffect",
      "createCompositeComputed",
      "createCompositeMemo",
      "createCompositeRenderEffect",
      "createModifier"
    ],
    "category": "Reactivity"
  },
  "private": false,
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "test": "uvu -r solid-register"
  },
  "keywords": [
    "watch",
    "effect",
    "composites",
    "computed",
    "memo",
    "solid",
    "primitives"
  ],
  "devDependencies": {
    "jsdom": "18.1.1",
    "prettier": "^2.0.5",
    "solid-register": "0.1.1",
    "tslib": "^2.0.1",
    "tsup": "^5.10.1",
    "typescript": "^4.5.2",
    "uvu": "^0.5.2"
  },
  "peerDependencies": {
    "solid-js": "^1.3.1"
  },
  "dependencies": {
    "@solid-primitives/debounce": "^1.1.0",
    "@solid-primitives/throttle": "^1.1.0"
  }
}