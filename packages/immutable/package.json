{
  "name": "@solid-primitives/immutable",
  "version": "0.1.8-beta.0",
  "description": "Functional programming helpers for making non-mutating changes to data. Keeping it immutable.",
  "author": "Damian Tarnawski @thetarnav <gthetarnav@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/solidjs-community/solid-primitives/tree/main/packages/immutable#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidjs-community/solid-primitives.git"
  },
  "primitive": {
    "name": "immutable",
    "stage": 2,
    "list": [
      "List of functions"
    ],
    "category": "Utilities"
  },
  "private": false,
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": {},
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "require": "./dist/index.cjs"
  },
  "scripts": {
    "build": "jiti ../../scripts/build.ts",
    "test": "vitest -c ../../configs/vitest.config.ts",
    "test:ssr": "pnpm run test --mode ssr"
  },
  "keywords": [
    "fp",
    "immutable",
    "solid",
    "primitives"
  ],
  "dependencies": {
    "@solid-primitives/utils": "^5.4.0-beta.0"
  },
  "peerDependencies": {
    "solid-js": "^1.6.0"
  },
  "typesVersions": {}
}
