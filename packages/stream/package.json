{
  "name": "@solid-primitives/stream",
  "version": "0.6.0",
  "description": "Primitive that gets a user media stream from microphone, camera or screen",
  "author": "Alex Lohr <alex.lohr@logmein.com>",
  "license": "MIT",
  "homepage": "https://github.com/solidjs-community/solid-primitives/tree/main/packages/stream",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidjs-community/solid-primitives.git"
  },
  "primitive": {
    "name": "stream",
    "stage": 3,
    "list": [
      "createStream",
      "createAmplitudeStream",
      "createMediaPermissionRequest",
      "createAmplitudeFromStream",
      "createScreen"
    ],
    "category": "Network"
  },
  "files": [
    "dist"
  ],
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "./dist/server.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "node": {
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    },
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "scripts": {
    "build": "tsup",
    "test": "uvu -r solid-register",
    "start": "vite serve dev --host",
    "dev": "npm run start",
    "demo": "vite serve demo --host"
  },
  "keywords": [
    "media",
    "stream",
    "solid",
    "primitives"
  ],
  "dependencies": {
    "@solid-primitives/utils": "workspace:^3.0.2"
  },
  "devDependencies": {
    "jsdom": "^20.0.0",
    "prettier": "^2.7.1",
    "solid-js": "^1.5.4",
    "solid-register": "^0.2.5",
    "tslib": "^2.4.0",
    "tsup": "^6.2.3",
    "typescript": "^4.8.2",
    "uvu": "^0.5.6",
    "vite": "^3.0.9",
    "vite-plugin-solid": "^2.3.0"
  },
  "peerDependencies": {
    "solid-js": ">= 1.3.0"
  }
}
